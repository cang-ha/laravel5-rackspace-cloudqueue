{
  "name": "cang-ha/laravel5-rackspace-cloudqueue",
  "description": "Rackspace CloudQueue driver for Laravel 5 Queue",
  "keywords": ["rackspace", "laravel", "laravel5", "queue", "cloud"],
  "homepage": "https://github.com/cang-ha/laravel5-rackspace-cloudqueue",
  "license": "MIT",
  "authors": [
    { 
      "name": "Cang Ho",
      "role": "Developer"
    },
    {
      "name": "Winter Faulk",
      "email": "winter@faulk.me",
      "homepage": "http://faulk.me",
      "role": "Developer"
    },
    {
      "name": "Russell Todd",
      "homepage": "https://github.com/npmweb/laravel-rackspace-cloudqueue",
      "role": "Developer"
    }
  ],
  "require": {
    "illuminate/queue": "5.*",
    "rackspace/php-opencloud": "1.*"
  },
  "autoload": {
    "psr-4": {
      "CangHA\\RackspaceCloudQueue\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "5.*",
    "orchestra/testbench": "~3.1",
    "mockery/mockery": "^0.9.5"
  },
  "autoload-dev": {
    "psr-4": {
      "CangHA\\RackspaceCloudQueue\\Tests\\": "tests/"
    }
  },
  "extra": {
        "laravel": {
            "providers": [
                "CangHA\\RackspaceCloudQueue\\RackspaceCloudQueueServiceProvider"
            ]
        }
  },
  "minimum-stability": "stable"
}
